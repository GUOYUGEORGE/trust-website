ate\";\n\n\nfunction App() {\n\n  useEffect(() => {\n    //Change title on tab\n    document.title = \"Tiaki Taonga Trust\";\n\n    //Change favicon\n    const link = document.querySelector(\"link[rel*='apple-touch-icon]\") || document.createElement('link');\n    link.type = 'image/x-icon';\n    link.rel = 'shortcut icon';\n    link.href = '/logo.png';\n    document.getElementsByTagName('head')[0].appendChild(link);\n\n  }, []);\n\n  return (\n    <>\n      <Router>\n        <Navbar />\n\n\n        <Routes>\n          <Route exact path='/trust-website/' element={<Home />} />\n          <Route path='/events' element={<Events />} />\n          <Route path='/about-us' element={<AboutUs />} />\n          <Route path='/donate' element={<Donate />} />\n        </Routes>\n      </Router>\n    </>\n        //Route path set on Navbar component\n  );\n\n}\n\nexport default App;","/home/guicosta/Odin Project/repos/trust-website/src/components/Navbar.js",["143"],[],"import React, {useEffect, useState} from 'react';\nimport {Link} from 'react-router-dom';\nimport { Button } from './Button';\nimport './Navbar.css'\n\nfunction Navbar() {\n    const [click, setClick] = useState(false);\n    const [button, setButton] = useState(true);\n\n    const handleClick = () => setClick(!click);\n    const closeMobileMenu = () => {setClick(false)};\n\n    const showButton = () => {\n\n        if(window.innerWidth <= 960) {\n            setButton(false);\n        } else {\n            setButton(true);\n        }\n    };\n\nuseEffect(() => {\n    showButton()\n}, []);\n\nwindow.addEventListener('resize', showButton);\n\n  return (\n    <>\n        <nav className='navbar'>\n            <div className='navbar-container'>\n                <Link to=\"/trust-website/\" className='navbar-logo' onClick={closeMobileMenu}>\n                    <img src='./ttt-logo.png'></img><p>Tiaki</p><p>Taonga</p><p>Trust</p>\n                </Link>\n                <div className='menu-icon' onClick={handleClick}>\n                    <i className={click ? 'fas fa-times' : 'fas fa-bars'}></i>\n                </div>\n                <ul className={click ? 'nav-menu active' : 'nav-menu'}>\n                    <li className='nav-item'>\n                        <Link to='/trust-website/' className='nav-links' onClick={closeMobileMenu}>\n                            Home\n                        </Link>\n                    </li>\n                    <li className='nav-item'>\n                        <Link to='/events' className='nav-links' onClick={closeMobileMenu}>\n                            Events\n                        </Link>\n                    </li>\n                    <li className='nav-item'>\n                        <Link to='/about-us' className='nav-links' onClick={closeMobileMenu}>\n                            About Us\n                        </Link>\n                    </li>\n                    <li className='nav-item'>\n                        <Link to='/donate' className='nav-links-mobile' onClick={closeMobileMenu}>\n                            Donate\n                        </Link>\n                    </li>\n                </ul>\n                {button && <Button buttonStyle='btn--outline'>DONATE</Button>}\n            </div>\n        </nav>\n\n    </>\n  )\n}\n\nexport default Navbar","/home/guicosta/Odin Project/repos/trust-website/src/components/pages/Home.js",[],[],"/home/guicosta/Odin Project/repos/trust-website/src/components/Button.js",["144"],[],"import React from 'react';\nimport './Button.css';\nimport {Link} from 'react-router-dom';\n\nconst STYLES = ['btn--primary', 'btn--outline']\n\nconst SIZES = ['btn--medium', 'btn--large']\n\nexport const Button = ({children, type, onClick, buttonStyle, buttonSize}) => {\n    const checkButtonStyle = STYLES.includes(buttonStyle) ? buttonStyle : STYLES [0]\n\n    const checkButtonSize = SIZES.includes(buttonSize) ? buttonSize : SIZES[0]\n\n    return (\n        <Link to='/donate' className='btn-mobile'>\n            <button className={`btn ${checkButtonStyle} ${checkButtonSize}`}\n                    onClick={onClick}\n                    type={type}\n                    >\n\n                {children}\n            </button>\n\n        \n\n        </Link>\n    )\n};","/home/guicosta/Odin Project/repos/trust-website/src/components/Cards.js",[],[],"/home/guicosta/Odin Project/repos/trust-website/src/components/Footer.js",[],[],"/home/guicosta/Odin Project/repos/trust-website/src/components/HeroSection.js",[],[],"/home/guicosta/Odin Project/repos/trust-website/src/components/CardItem.js",["145"],[],"import React from \"react\";\nimport {Link} from 'react-router-dom'\n\nfunction CardItem (props){\n    return (\n        <>\n        <li className=\"cards__item\">\n            <Link className=\"cards__item__link\" to={props.path}>\n                <figure className=\"cards__item__pic-wrap\" data-category={props.label}>\n                    <img src={props.src} alt='Travel Image'\n                    className=\"cards__item__img\" />\n                </figure>\n                <div className=\"cards__item__info\">\n                    <h5 className=\"cards__item__text\">{props.text}</h5>\n                </div>\n            </Link>\n        </li>\n        </>\n    )\n}\n\nexport default CardItem","/home/guicosta/Odin Project/repos/trust-website/src/components/pages/Donate.js",[],[],"/home/guicosta/Odin Project/repos/trust-website/src/components/pages/Events.js",[],[],"/home/guicosta/Odin Project/repos/trust-website/src/components/pages/AboutUs.js",[],[],"/home/guicosta/Odin Project/repos/trust-website/src/components/ContactUs.js",[],[],"/home/guicosta/Odin Project/repos/trust-website/src/components/AboutUsCard.js",["146","147","148"],[],"import React from \"react\"\nimport './AboutUsCard.css';\nimport myImage1 from '../assets/imagess/Discovering-New-Zealands-Maori-Culture-_-Image-Via-Trafalgar-800x450.jpg'\nimport myImage2 from '../assets/imagess/meg-jerrard-u7lgJ8qrdAM-unsplash.jpg'\nimport myImage3 from '../assets/imagess/duskfall-crew-vIQfQjrovW8-unsplash.jpg'\n\n\nfunction AboutUsCard() {\n    return (\n        <>\n        <div className=\"about-container\">\n          <div className=\"about-heading\">\n            <h1>What is the WAI 262 Claim</h1>\n            <p>The basic aspiration of the claim has often been coined by the phrase “Māori control over Māori things”.</p>\n          </div>\n\n          <section className=\"about-claim\">\n          <img src={myImage1} alt='Travel Image'\n                    className=\"about__img\" />\n            <div className=\"about-content\">\n              <h2>What is its purpose?</h2>\n              <p>The claim essentially seeks to restore “tino rangatiratanga” \n                (full authority and self‐determination) of the whānau, hapū \n                and iwi of Aotearoa over our “taonga” (those things and values \n                which we treasure, both intangible and tangible).\n              </p>\n              <button className=\"read-more-btn\">Read More</button>\n            </div>\n          </section>\n        </div>\n\n        <div className=\"about-container\">\n          <section className=\"about-claim\">\n          \n            <div className=\"about-content\">\n              <h2>How did it get started?</h2>\n              <p>After the claim was lodged in 1991, it was amended in 1993 to include international law matters\n                 such as the impact of international Trade Agreements (like the ‘Gatt: Trips Agreement’), Intellectual \n                 Property Rights (e.g. how Copyrights, Trademarks and Patents over taonga affect Māori control over \n                 the same), and New Zealand compliance with internationally-recognised Indigenous Peoples Rights.\n                 In 1997, the first Waitangi Tribunal hearing was held at Motuti, in the Far North. It took until \n                 2003 to hear all the \n                 claimant evidence around the motu, and it wasn’t until 2010 that the Waitangi Tribunal released the\n                  Reo Chapter of its \n                 report.  The Tribunal finally released its complete report, titled “Ko Aotearoa Tēnei” (‘This is\n                  Aotearoa’ or ‘This is \n                 New Zealand’), in 2011. By this time, Saana Murray of Ngāti Kuri was the only remaining claimant\n                  alive. Sadly, Saana \n                 passed away later that same year.\n              </p>\n              <button className=\"read-more-btn\">Read More</button>\n            </div>\n            <img src={myImage2} alt='Travel Image'\n                    className=\"about__img__center\" />\n          </section>\n        </div>\n\n\n        <div className=\"about-container\">\n          <section className=\"about-claim\">\n            <img src={myImage3} alt='Travel Image'\n                    className=\"about__img\" />\n            <div className=\"about-content\">\n              <h2>Where are we now?</h2>\n              <p>At the time of its release in 2011, the Crown promised to consider the Tribunal's report,\n                 but stated that it would not be rushed into responding. While the Crown admits the 262 report \n                 has far-reaching implications across a wide range of Government policies, iwi claimants are \n                 still waiting for the Crown's comprehensive response.\n              </p>\n              <button className=\"read-more-btn\">Read More</button>\n            </div>\n\n          </section>\n        </div>\n\n\n\n\n\n\n\n        </>\n\n  );\n}\n\nexport default AboutUsCard",{"ruleId":"149","severity":1,"message":"150","line":10,"column":75,"nodeType":"151","messageId":"152","endLine":10,"endColumn":85,"fix":"153"},{"ruleId":"154","severity":1,"message":"155","line":10,"column":21,"nodeType":"156","endLine":11,"endColumn":52},{"ruleId":"157","severity":1,"message":"158","line":3,"column":42,"nodeType":"159","messageId":"160","endLine":3,"endColumn":48},{"ruleId":"161","severity":1,"message":"162","line":33,"column":21,"nodeType":"156","endLine":33,"endColumn":47},{"ruleId":"149","severity":1,"message":"150","line":10,"column":75,"nodeType":"151","messageId":"152","endLine":10,"endColumn":85,"fix":"163"},{"ruleId":"154","severity":1,"message":"155","line":10,"column":21,"nodeType":"156","endLine":11,"endColumn":52},{"ruleId":"154","severity":1,"message":"155","line":18,"column":11,"nodeType":"156","endLine":19,"endColumn":46},{"ruleId":"154","severity":1,"message":"155","line":53,"column":13,"nodeType":"156","endLine":54,"endColumn":54},{"ruleId":"154","severity":1,"message":"155","line":61,"column":13,"nodeType":"156","endLine":62,"endColumn":46},"no-whitespace-before-property","Unexpected whitespace before property 0.","MemberExpression","unexpectedWhitespace",{"range":"164","text":"165"},"jsx-a11y/img-redundant-alt","Redundant alt attribute. Screen-readers already announce `img` tags as an image. You don’t need to use the words `image`, `photo,` or `picture` (or any specified custom words) in the alt prop.","JSXOpeningElement","no-unused-vars","'Switch' is defined but never used.","Identifier","unusedVar","jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.",{"range":"166","text":"165"},[344,345],"",[344,345]]